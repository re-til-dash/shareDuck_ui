{
  "name": "shareduck-ui",
  "version": "0.0.15",
  "type": "module",
  "author": "shareoluck",
  "repository": {
    "url": "https://github.com/re-til-dash/shareDuck_ui",
    "directory": "src/*"
  },
  "license": "MIT",
  "keywords": [
    "shareDuck",
    "shareoluck"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/es/index.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cjs/index.cjs.js",
      "require": "./dist/es/index.es.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -fr ./dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && chmod +x ./fixup.sh && ./fixup.sh && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "token-transformer": "node  ./src/config/tokenTransformer.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "cli-table3": "0.6.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.11"
  },
  "dependencies": {
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/styled-components": "^5.1.34",
    "prettier": "^3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.1.7",
    "@storybook/addon-essentials": "^8.1.7",
    "@storybook/addon-links": "^8.1.7",
    "@storybook/react": "^8.1.8",
    "@storybook/react-vite": "^8.1.7",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.1",
    "chromatic": "^11.5.4",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.1.0",
    "path": "^0.12.7",
    "storybook": "^8.1.7",
    "token-transformer": "^0.0.33",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-storybook": "^0.0.9",
    "vitest": "^1.6.0"
  },
  "packageManager": "yarn@4.4.0",
  "engines": {
    "yarn": "~4.4.0"
  }
}